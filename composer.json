    "keywords": ["jwt-auth","auth","token","generate token","login token"],
    "homepage": "https://github.com/nice-yu/jwt-auth",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "nice-yu",
            "email": "nice-yu@qq.com"
        }
